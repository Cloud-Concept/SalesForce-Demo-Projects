             box.setContentInnerHTML(&quot;&lt;a href=\&quot;#\&quot; onclick=\&quot;box.hide();\&quot;&gt;Close&lt;/a&gt;&lt;br/&gt;&lt;iframe src=\&quot;/apex/twitterConvoRefresh?id={!Twitter_Conversation__c.Id}\&quot; width=\&quot;100%\&quot; height=\&quot;250\&quot; border=\&quot;0\&quot; style=\&quot;border: 0\&quot;&gt;&lt;/iframe&gt;&quot;);

