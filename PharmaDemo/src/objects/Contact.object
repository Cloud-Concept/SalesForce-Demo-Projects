        <fullName>Advocacy__c</fullName>
        <description>Level of advocacy of this person: relationship times influence. Relationship x Influence = Advocacy. Relationship scores = (-2 to +2). Influence (0 to 3). Result/2 = # of arrows.</description>
        <externalId>false</externalId>
        <formula>CASE ( 

VALUE(CASE( Relationship__c , &quot;Excellent&quot;, &quot;2&quot;, &quot;Good&quot;, &quot;1&quot;, &quot;Neutral&quot;, &quot;0&quot;, &quot;Poor&quot;, &quot;-1&quot;, &quot;Hostile&quot;, &quot;-2&quot;, &quot;0&quot;) ) 

* 

VALUE (CASE(  Influence__c , &quot;Strong&quot;, &quot;3&quot;, &quot;Moderate&quot;, &quot;2&quot;, &quot;Weak&quot;, &quot;1&quot;, &quot;0&quot;) ) 

,

6, IMAGE(&quot;/servlet/servlet.FileDownload?file= 015j0000000e7fN&quot;,&quot;3 Up Arrows&quot;) , 
4, IMAGE(&quot;/servlet/servlet.FileDownload?file= 015j0000000e7fO&quot;,&quot;2 Up Arrows&quot;) ,
3, IMAGE(&quot;/servlet/servlet.FileDownload?file= 015j0000000e7fO&quot;,&quot;2 Up Arrows&quot;) ,
2, IMAGE(&quot;/servlet/servlet.FileDownload?file= 015j0000000e7fP&quot;,&quot;1 Up Arrow&quot;) ,
1, IMAGE(&quot;/servlet/servlet.FileDownload?file= 015j0000000e7fP&quot;,&quot;1 Up Arrow&quot;) ,
0, IMAGE(&quot;/servlet/servlet.FileDownload?file= 015j0000000e7fQ&quot;,&quot;Neutral Arrow&quot;) ,
-1, IMAGE(&quot;/servlet/servlet.FileDownload?file= 015j0000000e7fR&quot;,&quot;1 Down Arrow&quot;) ,
-2, IMAGE(&quot;/servlet/servlet.FileDownload?file= 015j0000000e7fR&quot;,&quot;1 Down Arrow&quot;) ,
-3, IMAGE(&quot;/servlet/servlet.FileDownload?file= 015j0000000e7fS&quot;,&quot;2 Down Arrows&quot;) ,
-4, IMAGE(&quot;/servlet/servlet.FileDownload?file= 015j0000000e7fS&quot;,&quot;2 Down Arrows&quot;) ,
-6, IMAGE(&quot;/servlet/servlet.FileDownload?file= 015j0000000e7fT&quot;,&quot;3 Down Arrows&quot;) ,
IMAGE(&quot;/servlet/servlet.FileDownload?file=015j0000000e7fQ&quot;,&quot;Neutral Arrow&quot;)

)</formula>
        <inlineHelpText>Visual scoring of the advocacy level of this person.</inlineHelpText>
