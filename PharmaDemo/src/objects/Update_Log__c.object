    <webLinks>
        <fullName>Apply_selected_updates</fullName>
        <availability>online</availability>
        <displayType>massActionButton</displayType>
        <linkType>javascript</linkType>
        <masterLabel>Apply selected updates</masterLabel>
        <openType>onClickJavaScript</openType>
        <protected>false</protected>
        <requireRowSelection>true</requireRowSelection>
        <url>{!REQUIRESCRIPT(&quot;/soap/ajax/13.0/connection.js&quot;)} 

var logRecords = {!GETRECORDIDS( $ObjectType.Update_Log__c)}; 
var newRecords = []; 

if (logRecords[0] == null) { 
   alert(&quot;Please select at least one record.&quot;);
} 
else { 
   var errors = []; 
   for (var n=0; n&lt;logRecords.length; n++) { 
         var r = new sforce.SObject(&quot;Update_Log__c&quot;); 
         r.id = logRecords[n]; 
         r.Update_applied__c = true; 
         newRecords.push(r);
   }
  var result = sforce.connection.update(newRecords); 
  if (result &amp;&amp; result.length){ 
       var numFailed = 0; 
       var numSucceeded = 0; 
       for (var i = 0; i &lt; result.length; i++){ 
             var res = result[i]; 
             if (res &amp;&amp; res.success == &apos;true&apos;){ 
                numSucceeded++; 
             } else { 
                var es = res.getArray(&quot;errors&quot;); 
                if (es.length &gt; 0) { 
                   errors.push(es[0].message); 
                } 
                numFailed++; 
             } 
       } 
       if (numFailed &gt; 0){ 
          alert(&quot;Failed: &quot; + numFailed + &quot;\nSucceeded: &quot; + numSucceeded + &quot; \n Due to: &quot; + errors.join(&quot;\n&quot;)); 
       } else { 
          alert(&quot;Number of updates applied: &quot; + numSucceeded); 
       } 
   }
   window.location.reload(); 
}</url>
    </webLinks>
